/ {
	#address-cells = <1>;
	#size-cells = <1>;

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;
		cpu@0 {
			clock-frequency = <0>;
			compatible = "riscv";
			device_type = "cpu";
			reg = <0>;
			riscv,isa = "rv32i";
		};
	};

	soc {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "spinal,saxon";
		ranges;


		uart0: uart@0 {
			compatible = "spinal,lib-com-uart-uartctrl";
			reg = <0xF0010000 0x1000>;
			label = "uart0";
			status = "disabled";
			current-speed = <0>;
			clock-frequency = <0>;
		};
	};
};
